<% flash.keys.each do |type| %>
  <div data-alert class="flash <%= type %> alert-box radius">
    <%= flash[type] %>
    <a href="#" class="close">&times;</a>
  </div>
<% end %>


<h1>Bookmarks</h1>
<ul>
  <% @bookmarks.each do |bookmark| %>
    <li><a href=<%=bookmark.url%>> <%= bookmark.title %> </a></li>
  <% end %>
</ul>
